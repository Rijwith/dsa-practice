   "source": [
    "def nextGreaterElement(nums):\n",
    "    n = len(nums)\n",
    "    result = [-1] * n\n",
    "    stack = [] \n",
    "\n",
    "    for i in range(n):\n",
    "        while stack and nums[i] > nums[stack[-1]]:\n",
    "            idx = stack.pop()\n",
    "            result[idx] = nums[i]\n",
    "        stack.append(i)\n",
    "\n",
    "    return result\n"
   ]
  }
